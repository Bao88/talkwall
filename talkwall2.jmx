          <stringProp name="script">import org.json.*;
import org.apache.jmeter.util.JMeterUtils;

// Retrieval of inter-thread data


//vars.put(&quot;ITERATION&quot;, vars.getIteration())


// place all code in try - catch for easier debugging!
try {
	// Retrieve transferred properties
	vars.put(&quot;WALL_ID&quot;, JMeterUtils.getProperty(&quot;WALL_ID&quot;));
	vars.put(&quot;QUESTION_LENGTH&quot;, JMeterUtils.getProperty(&quot;QUESTION_LENGTH&quot;));
	
	// Get question IDs
	int question_length = Integer.parseInt(vars.get(&quot;QUESTION_LENGTH&quot;));
	for( int i = 0; i &lt; question_length; i++ ) {
	    vars.put(&quot;QUESTION_&quot; + (i+1) + &quot;_ID&quot;, JMeterUtils.getProperty(&quot;QUESTION_&quot; + (i+1) + &quot;_ID&quot;));
	}

}
catch (Exception ex){
    log.warn(&quot;Error in my load saved variables&quot;, ex);
    throw ex; // elsewise JMeter will &quot;swallow&quot; the above exception
}







