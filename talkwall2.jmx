        <hashTree>
          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="Convert Wall JSON data" enabled="true">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">import org.apache.jmeter.util.JMeterUtils;
import org.json.*;

// Storage of data for inter-thread use

String data = prev.getResponseDataAsString();

// Convert result to Java JSON object   http://docs.oracle.com/javaee/7/api/javax/json/JsonObject.html

try {
	JSONObject jsonData = new JSONObject(data);
	JSONObject jsonWall = jsonData.getJSONObject(&quot;result&quot;);
	JSONArray jsonQuestions = jsonWall.getJSONArray(&quot;questions&quot;);
	String wall_id = jsonWall.getString(&quot;_id&quot;);
	
	// Store Wall ID
	vars.put(&quot;WALL_ID&quot;, wall_id);					// For use in this thread server calls
	JMeterUtils.setProperty(&quot;WALL_ID&quot;, wall_id);		// For transfer to other threads
	
	// Store Questions
	int questions_length = jsonQuestions.length();
	for (int i = 0; i &lt; questions_length; i++) {
		String qKey = &quot;QUESTION_&quot; + (i+1) + &quot;_ID&quot;;
		String _id = jsonQuestions.getJSONObject(i).getString(&quot;_id&quot;);
		vars.put(qKey, _id);
		JMeterUtils.setProperty(qKey, _id);
	}
	
	// Store the question count
	JMeterUtils.setProperty(&quot;QUESTION_LENGTH&quot;, Integer.toString(questions_length));
}
catch (Exception ex){
	log.warn(&quot;Error in my convert wall json data&quot;, ex);
	throw ex; // elsewise JMeter will &quot;swallow&quot; the above exception
}
</stringProp>
